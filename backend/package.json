{
  "name": "cleancare-backend",
  "version": "1.0.0",
  "description": "CleanCare E-commerce Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev-simple": "nodemon src/simple-server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "studio": "npx prisma studio",
    "download-images": "node scripts/download-sample-images.js",
    "setup": "npm run generate && npm run migrate && npm run download-images && npm run seed",
    "check": "node scripts/check-setup.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "@prisma/client": "^5.6.0",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "stripe": "^14.7.0",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "prisma": "^5.6.0"
  }
}